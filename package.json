{
   "name": "zep-js",
   "version": "0.1.1",
   "description": "Zep JS Client",
   "main": "dist/index.js",
   "module": "dist/index.esm.js",
   "types": "dist/index.d.ts",
   "scripts": {
      "build": "eslint src/* && tsc && tsc --module ESNext --outDir dist/esm",
      "start": "node dist/index.js",
      "dev": "ts-node src/main.ts",
      "clean": "rm -rf dist/*",
      "prepublishOnly": "npm run clean && npm run build",
      "prepare": "npm run build",
      "docbuild": "typedoc --out docs/ --excludePrivate --excludeInternal --excludeExternals src/"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/getzep/zep-client.git"
   },
   "keywords": [
      "zep",
      "client"
   ],
   "author": "Sharath Rajasekar <sharathr@gmail.com>",
   "license": "Apache 2.0",
   "dependencies": {
      "axios": "^1.4.0",
      "jaguarjs-jsdoc": "^1.0.2",
      "minami": "^1.2.3",
      "typescript": "^4.4.4"
   },
   "devDependencies": {
      "@types/node": "^16.9.1",
      "@typescript-eslint/eslint-plugin": "^5.59.5",
      "@typescript-eslint/parser": "^5.59.5",
      "eslint": "^8.40.0",
      "eslint-config-airbnb": "^19.0.4",
      "eslint-config-prettier": "^8.8.0",
      "eslint-plugin-import": "^2.27.5",
      "eslint-plugin-jsx-a11y": "^6.7.1",
      "eslint-plugin-prettier": "^4.2.1",
      "eslint-plugin-react": "^7.32.2",
      "eslint-plugin-react-hooks": "^4.6.0",
      "jsdoc": "^4.0.2",
      "prettier": "^2.8.8",
      "ts-node": "^10.2.1",
      "typedoc": "^0.24.7"
   },
   "files": [
      "dist"
   ]
}
